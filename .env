####
# 公共目录
BASE_PATH=
PHP_WWW_PATH=${BASE_PATH}/docker-server/htdocs/php

####
# Golang 配置
GOLANG_VERSION=1.20.0
CONTAINER_GOPATH=/data/htdocs/go
LOCALHOST_GOPATH=${BASE_PATH}/docker-server/htdocs/go
LOCALHOST_GOROOT=${BASE_PATH}/docker-server/server/golang/golang-1.20.0
GOLANG_DOWNLOAD_URL=

####
# Nginx 配置
NGINX_VERSION=1.21.0
NGINX_PATH=${BASE_PATH}/docker-server/server/nginx/nginx-1.21.0
NGINX_HTML=${BASE_PATH}/docker-server/server/nginx/nginx-1.21.0/html

####
# PHP 5.6.40 配置
PHP56_VERSION=5.6.40
PHP56_PATH=${BASE_PATH}/docker-server/server/php/php-5.6.40
PHP56_SITE_LOG_DIR=${BASE_PATH}/docker-server/logs/web-logs

####
# PHP 7.3.33 配置
PHP73_VERSION=7.3.33
PHP73_PATH=${BASE_PATH}/docker-server/server/php/php-7.3.33
PHP73_SITE_LOG_DIR=${BASE_PATH}/docker-server/logs/web-logs

####
# PHP 7.4.22 配置
PHP74_VERSION=7.4.22
PHP74_PATH=${BASE_PATH}/docker-server/server/php/php-7.4.22
PHP74_SITE_LOG_DIR=${BASE_PATH}/docker-server/logs/web-logs

####
# PHP 8.1.0 配置
PHP8_VERSION=8.1.0
